{
  "manifest_version": 3,
  "name": "NewTab Uygulaması",
  "version": "2.1.4",
  "description": "Yeni sekme için özel arayüz",
  "permissions": [
    "history",
    "tabs",
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "Yeni Sekme"
  },
  "host_permissions": [
    "https://api.openweathermap.org/*",
    "https://translate.google.com/",
    "https://www.youtube.com/*",
    "https://mehasoft.com.tr/*"
  ]
}
