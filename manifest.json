{
  "manifest_version": 3,
  "name": "Yeni Sekme Uygulaması",
  "version": "1.0.16",
  "description": "Yeni sekme için özel arayüz",
  "permissions": [
    "bookmarks"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_title": "Yeni Sekme"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}